{
  "name": "yone-discord-bot",
  "version": "1.0.0",
  "description": "Discord bot built with TypeScript and Discord.js",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "vitest run --reporter=verbose",
    "test:once": "vitest run --reporter=verbose",
    "test:watch": "vitest",
    "test:silent": "vitest run --silent",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": ["discord", "bot", "typescript"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "discord.js": "^14.16.3"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.5.4",
    "tsx": "^4.16.2",
    "vitest": "^2.0.5",
    "@vitest/coverage-v8": "^2.0.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}