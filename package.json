{
  "name": "discord-bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/ tests/ --ext .ts",
    "lint:prod": "eslint src/ --ext .ts",
    "lint:fix": "eslint src/ tests/ --ext .ts --fix",
    "check": "npm run typecheck && npm run lint",
    "check:prod": "npm run typecheck && npm run lint:prod",
    "build": "npm run check:prod && tsc && npm run build-commands",
    "build-only": "tsc",
    "build-commands": "tsx src/scripts/build-commands.ts",
    "build-commands:prod": "tsx src/scripts/build-commands.ts --production",
    "build-commands:check": "tsx src/scripts/build-commands.ts --check",
    "build-commands:dry": "tsx src/scripts/build-commands.ts --dry-run",
    "test-discovery": "npx tsx src/scripts/test-discovery.ts",
    "test": "vitest run",
    "dev": "npm run check && tsx src/index.ts",
    "deploy-commands": "tsx src/scripts/deploy-commands.ts",
    "verify-auth": "node src/scripts/verify-google-auth.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "discord.js": "^14.21.0",
    "dotenv": "^17.2.0",
    "eslint": "^8.57.1",
    "express": "^4.18.2",
    "google-auth-library": "^9.0.0",
    "googleapis": "^126.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^24.0.15",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
